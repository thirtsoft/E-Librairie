{
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/*.css",
          "/*.js"
        ]
      }
    }, {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "apiCategorie",
      "urls": [
        "http://localhost:8081/api/**"
      ],
      "cacheConfig": {
        "maxSize": 100,
        "maxAge": "2d",
        "timeout": "20s",
        "strategy": "freshness"
      }
    }
  ]
}
